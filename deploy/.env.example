# GameCreatorMVP Environment Variables
# Copy this to .env and fill in your values

# Gemini API Key (for code generation)
GEMINI_API_KEY=your_gemini_api_key_here

# Anthropic API Key (for Claude CLI - set via 'claude config')
# Run: claude config set api_key your_anthropic_api_key

# Server port (default: 3000)
PORT=3000

# Node environment
NODE_ENV=production
